#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/victorndirangu/Documents/gitlab/tests/RoutingDemo/node_modules/.pnpm/@angular+compiler-cli@16.2.12_@angular+compiler@16.2.12_typescript@5.1.6/node_modules/@angular/compiler-cli/bundles/src/bin/node_modules:/Users/victorndirangu/Documents/gitlab/tests/RoutingDemo/node_modules/.pnpm/@angular+compiler-cli@16.2.12_@angular+compiler@16.2.12_typescript@5.1.6/node_modules/@angular/compiler-cli/bundles/src/node_modules:/Users/victorndirangu/Documents/gitlab/tests/RoutingDemo/node_modules/.pnpm/@angular+compiler-cli@16.2.12_@angular+compiler@16.2.12_typescript@5.1.6/node_modules/@angular/compiler-cli/bundles/node_modules:/Users/victorndirangu/Documents/gitlab/tests/RoutingDemo/node_modules/.pnpm/@angular+compiler-cli@16.2.12_@angular+compiler@16.2.12_typescript@5.1.6/node_modules/@angular/compiler-cli/node_modules:/Users/victorndirangu/Documents/gitlab/tests/RoutingDemo/node_modules/.pnpm/@angular+compiler-cli@16.2.12_@angular+compiler@16.2.12_typescript@5.1.6/node_modules/@angular/node_modules:/Users/victorndirangu/Documents/gitlab/tests/RoutingDemo/node_modules/.pnpm/@angular+compiler-cli@16.2.12_@angular+compiler@16.2.12_typescript@5.1.6/node_modules:/Users/victorndirangu/Documents/gitlab/tests/RoutingDemo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/victorndirangu/Documents/gitlab/tests/RoutingDemo/node_modules/.pnpm/@angular+compiler-cli@16.2.12_@angular+compiler@16.2.12_typescript@5.1.6/node_modules/@angular/compiler-cli/bundles/src/bin/node_modules:/Users/victorndirangu/Documents/gitlab/tests/RoutingDemo/node_modules/.pnpm/@angular+compiler-cli@16.2.12_@angular+compiler@16.2.12_typescript@5.1.6/node_modules/@angular/compiler-cli/bundles/src/node_modules:/Users/victorndirangu/Documents/gitlab/tests/RoutingDemo/node_modules/.pnpm/@angular+compiler-cli@16.2.12_@angular+compiler@16.2.12_typescript@5.1.6/node_modules/@angular/compiler-cli/bundles/node_modules:/Users/victorndirangu/Documents/gitlab/tests/RoutingDemo/node_modules/.pnpm/@angular+compiler-cli@16.2.12_@angular+compiler@16.2.12_typescript@5.1.6/node_modules/@angular/compiler-cli/node_modules:/Users/victorndirangu/Documents/gitlab/tests/RoutingDemo/node_modules/.pnpm/@angular+compiler-cli@16.2.12_@angular+compiler@16.2.12_typescript@5.1.6/node_modules/@angular/node_modules:/Users/victorndirangu/Documents/gitlab/tests/RoutingDemo/node_modules/.pnpm/@angular+compiler-cli@16.2.12_@angular+compiler@16.2.12_typescript@5.1.6/node_modules:/Users/victorndirangu/Documents/gitlab/tests/RoutingDemo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bundles/src/bin/ngc.js" "$@"
else
  exec node  "$basedir/../../bundles/src/bin/ngc.js" "$@"
fi
